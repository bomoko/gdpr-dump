{
  "name": "machbarmacher/gdpr-dump",
  "description": "A drop-in replacement for mysqldump that optionally sanitizes DB fields for better GDPR conformity.",
  "require": {
    "symfony/console": "^3.0",
    "ifsnop/mysqldump-php": "dev-master",
    "cweagans/composer-patches": "^1.6",
    "fzaninotto/faker": "^1.7",
    "bomoko/mysql-cnf-parser": "^0.0.1"
  },
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Axel Rutz",
      "email": "axel.rutz@machbarmacher.net"
    },
    {
      "name": "Blaize Kaye",
      "email": "blaize.kaye@amazee.com"
    },
    {
      "name": "Fran Garc√≠a-Linares",
      "email": "fran.garcia@amazee.com"
    }
  ],
  "bin": [
    "mysqldump"
  ],
  "autoload": {
    "psr-4": {
      "machbarmacher\\GdprDump\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phpunit/phpunit": "^7.1"
  }
}
